	<queryString language="SQL">
		<![CDATA[SELECT 
    o.orderID,
    o.customerID AS order_customerID,
    c.NIC AS customer_NIC,
    c.customerName AS customer_name,
    c.customerAddress AS customer_address,
    c.customerTel AS customer_tel,
    e.employeeID,
    e.NIC AS employee_NIC,
    e.employeeName AS employee_name,
    e.phoneNumber AS employee_phone,
    e.employeeAddress AS employee_address,
    e.salary AS employee_salary,
    od.fabricID,
    od.description AS fabric_description,
    od.measurements AS fabric_measurements,
    od.fabricSize AS fabric_size,
    od.unitPrice AS fabric_unit_price,
    od.qty AS fabric_qty,
    f.fabricName,
    f.fabricColor,
    f.fabricQtyOnHand,
    p.paymentType,
    p.price
FROM 
    orders o
    JOIN customer c ON o.customerID = c.customerID
    JOIN employee e ON o.employeeID = e.employeeID
    JOIN orderDetails od ON o.orderID = od.orderID
    JOIN fabric f ON od.fabricID = f.fabricID
    JOIN payment p ON o.paymentID = p.paymentID
    WHERE 
    o.orderID = $P{orderID}]]>
	</queryString>
